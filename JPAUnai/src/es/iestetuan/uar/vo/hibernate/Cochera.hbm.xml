<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="es.iestetuan.dhs.vo.Cochera" table="t_cochera">
        <id name="codigoCochera" type="java.lang.Integer" column="cod_cochera">
            <generator class="identity" />
        </id>

        <property name="nombre" column="nombre" type="java.lang.String"/>
        <property name="direccion" column="direccion" type="java.lang.String"/>
        <property name="deposito" column="deposito" type="java.lang.Integer"/>
        
        
    </class>
</hibernate-mapping>